{
  "name": "cacao-api",
  "version": "0.1.0",
  "description": "Express App with Typescript and Prisma ORM for the cacao-dashboard",
  "main": "./dist/index.js",
  "repository": "https://gitlab.com/internal51/choco4peace/cacao-api.git",
  "author": "razvan-mrc <87325253+razvan-mrc@users.noreply.github.com>",
  "license": "MIT",
  "prisma": {
    "seed": "ts-node scripts/seed.ts"
  },
  "_moduleAliases": {
    "@modules": "build/modules",
    "@core": "build/core"
  },
  "scripts": {
    "start:docker": "yarn build && node ./dist/src/index.js",
    "docker:db:migrate": "docker compose run cacao-api yarn prisma migrate dev",
    "docker:db:seed": "docker compose run cacao-api yarn prisma db seed",
    "docker": "docker-compose up --build",
    "start": "ts-node ./src/index.ts",
    "dev": "nodemon ./src/index.ts",
    "build": "tsc",
    "prisma": "prisma",
    "seed": "yarn prisma db seed"
  },
  "dependencies": {
    "@prisma/client": "^4.14.0",
    "@types/node-cron": "^3.0.7",
    "axios": "^1.4.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "exceljs": "^4.3.0",
    "express": "^4.18.2",
    "joi": "^17.9.2",
    "jsonwebtoken": "^9.0.0",
    "jszip": "^3.10.1",
    "node-cron": "^3.0.2",
    "nodemailer": "^6.9.3",
    "viem": "^0.3.30"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^4.1.1",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/node": "^20.1.1",
    "@types/nodemailer": "^6.4.8",
    "@types/nodemon": "^1.19.2",
    "@typescript-eslint/eslint-plugin": "^5.59.5",
    "@typescript-eslint/parser": "^5.59.5",
    "eslint": "^8.40.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.8",
    "prisma": "^4.14.0",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.0.4"
  }
}
